<?xml version="1.0" encoding="utf-8"?>
<com.example.renhao.wevolunteer.view.FractionTranslateLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <com.handmark.pulltorefresh.library.PullToRefreshListView
        android:id="@+id/ptrListview_homapage_list"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#ffeaebeb"
        android:cacheColorHint="#00000000"
        android:choiceMode="singleChoice"
        android:divider="#ffffffff"
        android:dividerHeight="8dp"
        android:drawSelectorOnTop="false"
        android:fastScrollEnabled="false"
        android:footerDividersEnabled="false"
        android:groupIndicator="@null"
        android:listSelector="@drawable/timer_list_selector"
        android:scrollingCache="true"
        app:ptrHeaderBackground="#ffeaebeb"/>

</com.example.renhao.wevolunteer.view.FractionTranslateLayout>